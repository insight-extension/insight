{
  "name": "extension",
  "description": "__MSG_extensionDescription__",
  "version": "0.0.6",
  "author": "Insight's team",
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.13.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@repo/shared": "workspace:*",
    "@repo/ui": "workspace:*",
    "@solana/web3.js": "^1.95.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "debounce": "^2.2.0",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.22",
    "lucide-react": "^0.460.0",
    "plasmo": "0.89.4",
    "react": "18.3.1",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.5.0"
  },
  "resolutions": {
    "svgo": "^3.0.2"
  },
  "devDependencies": {
    "@types/chrome": "0.0.258",
    "@types/node": "22.7.4",
    "@types/react": "18.3.1",
    "@types/react-dom": "^18.3.1",
    "copyfiles": "^2.4.1",
    "csstype": "^3.1.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "undici-types": "^6.19.8"
  },
  "displayName": "__MSG_extensionName__",
  "manifest": {
    "name": "INSIGHT Extension",
    "key": "$CRX_ID",
    "default_locale": "en",
    "host_permissions": [
      "https://*/*",
      "http://*/*",
      "wss://*/*",
      "ws://*/*"
    ],
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "tabCapture",
      "sidePanel",
      "scripting",
      "cookies"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "pcm-processor.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "externally_connectable": {
      "matches": [
        "<all_urls>"
      ]
    }
  },
  "plasmo": {
    "static": [
      "processors"
    ]
  },
  "scripts": {
    "build": "plasmo build && copyfiles -u 1 src/pcm-processor.js build/chrome-mv3-prod/",
    "dev": "plasmo dev && copyfiles -u 1 src/pcm-processor.js build/chrome-mv3-dev/",
    "package": "plasmo package"
  }
}
